{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "baseUrl": ".", // Required for "paths" to work
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@types/*": ["src/types/*"],
      "@locales/*": ["locales/*"],
      "@constants/*": ["src/constants/*"],
      "@hooks/*": ["src/hooks/*"],
      "@utils/*": ["src/utils/*"],
      "@context/*": ["src/context/*"],
      "@wrapper/*": ["src/wrapper/*"]
    }
  }
}