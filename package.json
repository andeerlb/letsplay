{
  "name": "letsplay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "cross-env ENVFILE=.env.dev react-native run-android",
    "android:demo": "ENVFILE=.env.demo react-native run-android",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios",
    "ios:demo": "ENVFILE=.env.demo react-native run-ios",
    "lint": "eslint .",
    "start": "yarn i18n:update && react-native start",
    "start:no-cache": "yarn i18n:update && npx react-native start --reset-cache",
    "test": "jest",
    "postinstall": "patch-package",
    "i18n:update": "lingui extract && lingui compile",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "android-clean": "rm -rf node_modules android/app/build android/.cxx && yarn cache clean && yarn install && cd android && ./gradlew clean && cd .. && npx react-native run-android",
    "ios-clean": "rm -rf node_modules ios/Pods ios/Podfile.lock ios/build && yarn cache clean && yarn install && cd ios && pod install --repo-update && cd .. && npx react-native run-ios"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@lingui/core": "^5.3.3",
    "@lingui/react": "^5.3.3",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.83.0",
    "lottie-react-native": "^7.2.4",
    "react": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.80.1",
    "react-native-config": "^1.5.5",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-masked-text": "^1.13.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.0",
    "react-native-svg": "^15.12.0",
    "react-redux": "^9.2.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@lingui/babel-plugin-lingui-macro": "^5.3.3",
    "@lingui/metro-transformer": "^5.3.3",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "miragejs": "^0.1.48",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=18"
  }
}
